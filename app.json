{
  "expo": {
    "name": "Airbnb-Clone",
    "slug": "Airbnb-Clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/applogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.reactnativeclones.airbnb",
      "supportsTablet": true
    },
    "android": {
      "package": "com.reactnativeclones.airbnb",
      "adaptiveIcon": {
        "foregroundImage": "./assets/applogo.png"
      }
    },
    "web": {
      "favicon": "./assets/applogo.png"
    }
  }
}
